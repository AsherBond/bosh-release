daemonize no
pidfile /var/vcap/sys/run/redis/redis.pid
port <%= p("redis.port") %>
<% if_p("redis.listen_address") do |listen_address| %>
bind <%= listen_address %>
<% end %>
timeout 300
loglevel notice
logfile stdout
databases 16
dir /var/vcap/store/redis
requirepass <%= p("redis.password") %>
maxclients 0
maxmemory-policy noeviction
appendonly no
